@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .

@prefix lgdo: <http://linkedgeodata.org/ontology/> .
@prefix wgs:  <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix dcterms: <http://purl.org/dc/terms/> .


wgs:geometry
	a owl:DatatypeProperty .

lgdo:hasNodes
	a owl:ObjectProperty ;
	rdfs:comment "This property connects an object with an ordered sequence of nodes. Note that this relation is not meant to be transitive: If A is a part of B, and A 'hasNodes', then B does not have these nodes." ;
	rdfs:range rdf:Seq .


lgdo:Node
    a owl:Class ;
	owl:equivalentClass lgdo:NodeRestriction .

lgdo:NodeRestriction
	a owl:Restriction ;
	owl:onProperty wgs:geometry ;
	owl:someValuesFrom rdfs:Literal .


lgdo:Way
    a owl:Class ;
	owl:equivalentClass lgdo:WayRestriction .

lgdo:WayRestriction
	a owl:Restriction ;
	owl:onProperty lgdo:hasNodes ;
	owl:someValuesFrom owl:Thing .

lgdo:schemaIcon
	a owl:AnnotationProperty .
	
lgdo:contributor
	a owl:AnnotationProperty .
	
	

	